@model CMS_Brian.Models.Admin.UserResponse.ClientDetail
@using CMS_Brian.Models

@{
    ViewBag.Title = "ClientDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<header class="ribbon">
    <h2>&nbsp;</h2>
</header>

<div class="page-content">
    <div class="container-fluid">
        @{Html.RenderPartial("alerts");}
        <div class="kt-portlet kt-portlet--mobile">
            <div class="kt-portlet__head kt-portlet__head--lg breadcrumb-sec">
                <div class="kt-portlet__head-label">
                    <h4>
                        <a href="/Admin/Client" class="kt-menu__link ">
                            <i class="kt-menu__link-icon flaticon-users"><span></span></i>
                            <span class="kt-menu__link-text">Clients</span>
                        </a>
                        <span> > &nbsp; </span>
                    </h4>
                    <h3 class="kt-portlet__head-bred">
                        Details
                    </h3>
                </div>
                <div class="kt-portlet__head-toolbar">
                    <div class="kt-portlet__head-wrapper">
                        <div class="kt-portlet__head-actions">
                            <a href="/admin/Client" class="btn btn-brand btn-elevate btn-icon-sm">
                                <i class="la la-step-backward"></i>
                                Back
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="kt-portlet__body">
                <div class="kt-card-profile__details">
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>First Name : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.FirstName</b>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <img src="@Model.SignedForm" onerror="changeurl(this)" class="kt-card-profile__no-photo" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>LastName : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.LastName</b>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>Gender : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.Gender</b>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>Phone Number : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.PhoneNumber</b>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>Date of Birth : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.DateofBirth</b>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>JudgeName : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.JudgeName</b>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>Status : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.Status</b>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                            <div class="form-group">
                                <strong><label>Address : </label></strong>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="form-group">
                                <b> @Model.Address</b>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="kt-portlet__foot">
                <div class="kt-form__actions">
                    <a href="/Admin/AddClientsClass/?clientId=@Model.ClientId" class="btn btn-brand btn-elevate btn-icon-sm">
                        <i class="la la-plus"></i>
                        Assign Classes
                    </a>
                </div>
            </div>
            <div class="container-fluid">
                @{Html.RenderPartial("alerts");}
                <input type="hidden" id="PageView" value="ClientClassesPartialView/?ClientId=@Model.ClientId" />
                <div id="main-content">
                </div>
            </div>

        </div>
    </div>
</div>
@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            var url = $("#PageView").val();
            console.log(url);
            admin.LoadAdminPartialView(url,"#main-content");
        });
    </script>
}